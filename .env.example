# Discord configuration
DISCORD_TOKEN=your-discord-bot-token
DISCORD_GUILD_ID=your-server-id  # Right click server icon -> Copy Server ID
COLLECT_SINCE_DATE=2025-02-06  # Format: YYYY-MM-DD

# Excluded channels are configured in discord_collector.py:
# - rules
# - moderator-only
# - welcome
# - announcements
# - featured
# - jobs
# - conferences
# - hackathons

# AI Model configuration
# Choose between 'openai' or 'gemini'
AI_PROVIDER=openai

# OpenAI API Key (required if AI_PROVIDER=openai)
OPENAI_API_KEY=your-openai-api-key-here

# Gemini API Key (required if AI_PROVIDER=gemini)
# GEMINI_API_KEY=your-gemini-api-key-here

# Output configuration
OUTPUT_DIR=output

# Optional: Configure proxy settings for web scraping if needed
# HTTP_PROXY=http://proxy:port
# HTTPS_PROXY=http://proxy:port

# Crawler settings
HEADLESS=true
TIMEOUT=30
